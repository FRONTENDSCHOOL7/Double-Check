{
  "extends": [
    "prettier",
    "eslint:recommended",
    "react-app",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-var": "error",
    "require-await": "error",
    "eqeqeq": "warn",
    "no-unused-vars": "off",
    "prettier/prettier": "error"
  }
}
